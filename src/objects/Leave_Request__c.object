    <validationRules>
        <fullName>Applying_Leave_Should_be_in_Current_Year</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Override_Validation__c =false ,OR(YEAR( Start_Date__c )  &lt;&gt;  YEAR( TODAY() ) ,YEAR( End_Date__c )&lt;&gt; YEAR(TODAY())  ))</errorConditionFormula>
        <errorMessage>Year must be in the current year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDate_should_be_greterthan_StartDate</fullName>
        <active>false</active>
        <errorConditionFormula>Start_Date__c  &gt; End_Date__c</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start Date should not be lessthan End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Leaves_Balance_Cannot_Be_Nagative</fullName>
        <active>true</active>
        <errorConditionFormula>Total_Working_days__c  &lt;0</errorConditionFormula>
        <errorDisplayField>Total_Working_days__c</errorDisplayField>
        <errorMessage>Employee don&apos;t have enough leave balance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Optional_Holiday</fullName>
        <active>true</active>
        <description>Start date and end date should be equal .</description>
        <errorConditionFormula>AND(Optional_Holiday__c=TRUE , Override_Validation__c  = FALSE,End_Date__c &lt;&gt; Start_Date__c   )</errorConditionFormula>
        <errorDisplayField>Optional_Holiday__c</errorDisplayField>
        <errorMessage>To apply for optional holiday the start date and End date should be same  ,</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Optional_Holiday_Count</fullName>
        <active>false</active>
        <description>Employee cannot take more than  two Optional Holidays in Current calendar year</description>
        <errorConditionFormula>$User.Optional_Holiday_Count__c &gt;=  2</errorConditionFormula>
        <errorMessage>Employee cannot take more than two  Optional Holidays in current calendar year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Leave_Status</fullName>
        <active>false</active>
        <description>Status cannot made to approve/cancelled  ,before  applying leave</description>
        <errorConditionFormula>AND(  ISNEW() , OR( ISPICKVAL( Status__c , &apos;Approved&apos;),ISPICKVAL( Status__c , &apos;Cancelled&apos;)), Override_Validation__c =FALSE)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status cannot made to approve/cancelled  ,before  applying leave</errorMessage>
    </validationRules>
</CustomObject>
