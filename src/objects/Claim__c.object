    <validationRules>
        <fullName>Validation_on_Approver_Manager</fullName>
        <active>true</active>
        <errorConditionFormula>Approving_Manager__c = OwnerId</errorConditionFormula>
        <errorDisplayField>Approving_Manager__c</errorDisplayField>
        <errorMessage>Client Owner cannot be Approver Manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Submitters_Comment</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(Claim_Status__c ,&quot;Submitted&quot;), ISBLANK( Submitter_Comments__c))</errorConditionFormula>
        <errorDisplayField>Submitter_Comments__c</errorDisplayField>
        <errorMessage>Submitter&apos;s Comment needs to be filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_rule_to_Edit_Claim</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OwnerId!= $User.Id,ISPICKVAL(Claim_Status__c, &quot;Saved&quot;))</errorConditionFormula>
        <errorMessage>Claim Owner can only edit the Claim records</errorMessage>
    </validationRules>
