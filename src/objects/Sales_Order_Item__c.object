    <validationRules>
        <fullName>Status_cant_be_made_Pending_without_Plan</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISBLANK(Planned_Date__c), ISPICKVAL( Status__c ,&apos;Pending&apos;))</errorConditionFormula>
        <errorDisplayField>Planned_Date__c</errorDisplayField>
