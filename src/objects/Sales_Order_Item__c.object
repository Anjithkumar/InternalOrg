    </validationRules>
    <validationRules>
        <fullName>Status_completed_Actual_date_Mandatory</fullName>
        <active>false</active>
        <description>If Milestone Status is Complete then Actual date should be Populated.</description>
        <errorConditionFormula>AND(ISPICKVAL(  Status__c , &apos;Completed&apos;) , ISNULL( Actual_Date__c ))</errorConditionFormula>
        <errorDisplayField>Actual_Date__c</errorDisplayField>
        <errorMessage>Actual Date Is Needed If Status Is Completed</errorMessage>
    </validationRules>
