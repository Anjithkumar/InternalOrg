else if( taxCode==null || taxCode==&apos;&apos;||taxPercent==&apos;&apos;||taxPercent==null)
{ 
alert(&apos;Missing Tax Code and Tax Percent for Salesorder&apos;);
}
else
{
for(var i=0;i&lt;selectedRecordids.length;i++){
