    <validationRules>
        <fullName>Amount_Field_Can_Not_Be_Blank</fullName>
        <active>false</active>
        <description>Based upon the cost picklist selection corresponding Amount field should be filled.</description>
        <errorConditionFormula>OR( AND(ISPICKVAL(Cost__c,&apos;License Only&apos;), ISBLANK( License_Amount__c ) ),
        AND(ISPICKVAL(Cost__c,&apos;Service Only&apos;), ISBLANK(  Service_Amount__c  ) ),
         AND(ISPICKVAL(Cost__c,&apos;Both&apos;), OR(ISBLANK(  Service_Amount__c  ), ISBLANK(  License_Amount__c ) ) )


 )</errorConditionFormula>
        <errorMessage>Amount Should Be Filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Start_Date_Can_Not_Be_Blank</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName ,&apos;Closed Won&apos;),( ISBLANK( Contract_Start_Date__c ) ),ISPICKVAL(  Cost__c ,&apos;License Only&apos;))</errorConditionFormula>
        <errorMessage>Contract Start Date Is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Status_Can_Not_Be_Blank</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName ,&apos;Closed Won&apos;),( ISBLANK( TEXT( Status__c )  )),ISPICKVAL(  Cost__c ,&apos;License Only&apos;))</errorConditionFormula>
        <errorMessage>Status Is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Term_Months_can_not_be_blank</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName ,&apos;Closed Won&apos;),ISPICKVAL(  Cost__c ,&apos;License Only&apos;),( ISBLANK(  Contract_Term_Month__c ) ))</errorConditionFormula>
        <errorMessage>Contract Term(Month) Is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_should_be_accepted_by_customer</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  ISPICKVAL( StageName ,&apos;Closed Won&apos;), Quote_Accepted_By_Customer__c ==false  ,ISPICKVAL(  Cost__c ,&apos;License Only&apos;))</errorConditionFormula>
        <errorMessage>Quote should be accepted by customer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Should_Be_Filled_When_Closed_Lost</fullName>
        <active>false</active>
        <description>Reason to be filled when moving opportunity to Closed Lost</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName ,&apos;Closed Lost&apos;), ISBLANK( Reason_for_Closed_Lost__c ) )</errorConditionFormula>
        <errorMessage>Please fill the reason for Closed/Lost</errorMessage>
    </validationRules>
