        <externalId>false</externalId>
        <formula>IF(And(!ISBLANK(Start_Date__c),!ISBLANK(End_Date__c),(Start_Date__c= End_Date__c )),(Effort_Days__c/1),
IF(And(!ISBLANK(Start_Date__c),!ISBLANK(End_Date__c),!ISBLANK(Effort_Days__c)), 
