    <validationRules>
        <fullName>Same_Opportunity_can_be_only_one</fullName>
        <active>false</active>
        <description>Same Opportunity cannot be commit, Best case &amp; Omitted.</description>
        <errorConditionFormula>If(Best_Case__c==True &amp;&amp; (Commit__c ||Omitted__c ),True,
If(Commit__c==True &amp;&amp; (Best_Case__c ||Omitted__c ),True,
If(Omitted__c==True &amp;&amp; (Best_Case__c || Commit__c ),True,
False)))</errorConditionFormula>
        <errorMessage>Only One CheckBox (Commit,Best Case or Omitted) Can be true at a time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Startdate_should_be_greater_than_Enddate</fullName>
        <active>false</active>
        <errorConditionFormula>Deal_Start_Date__c  &gt; Deal_End_Date__c</errorConditionFormula>
        <errorDisplayField>Deal_Start_Date__c</errorDisplayField>
        <errorMessage>Contract Start Date:Contract Start Date should be lessthan Contract End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Contract_Start_Date</fullName>
        <active>false</active>
        <description>Validation on Contract Start Date, when Deal is closed Won.</description>
        <errorConditionFormula>if(AND(RecordType.DeveloperName=&apos;Licenses&apos;, ISPICKVAL(Stage__c,&quot;Closed Won&quot;)),
ISBLANK(Deal_Start_Date__c),false)</errorConditionFormula>
        <errorDisplayField>Deal_Start_Date__c</errorDisplayField>
        <errorMessage>Once Deal is Closed won, Contract Start Date is Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Deal_Stage</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(Stage__c ,&quot;Closed Won&quot;), Quote_Accepted_By_Customer__c =false)</errorConditionFormula>
        <errorDisplayField>Quote_Accepted_By_Customer__c</errorDisplayField>
        <errorMessage>We cannot close deal without customer accepting quote</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Stage_Probability</fullName>
        <active>false</active>
        <description>If Probability is &gt;70%,then atleast one resource should be allocated(In Resource Forecast 1 record should be created )</description>
        <errorConditionFormula>if(AND( Deal_Type__c=&apos;Services&apos;,OR( ISPICKVAL( Stage__c , &apos;Proposal/Price Quote&apos;), ISPICKVAL(Stage__c, &apos;Negotiation/Review&apos;),ISPICKVAL(Stage__c, &apos;Closed Won&apos;))),Resource_Forecast_Count__c = 0,false)</errorConditionFormula>
        <errorMessage>If Probability is &gt;70% ,then atleast one Resource should be allocated to the Deal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Total_Estimated_Hours</fullName>
        <active>false</active>
        <errorConditionFormula>IF(AND(RecordType.DeveloperName=&apos;Services&apos;, Quote_Accepted_By_Customer__c =true), ISBLANK( Total_Estimated_Hours__c ) , false)</errorConditionFormula>
        <errorDisplayField>Total_Estimated_Hours__c</errorDisplayField>
        <errorMessage>Once Quote is accepted by Customer, Total Estimated Hours should be mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_stage_closed_lost</fullName>
        <active>false</active>
        <description>When the stage is &quot;Closed lost &quot;, the  field &quot;reason for the lost &quot; should not be empty</description>
        <errorConditionFormula>AND(ISPICKVAL( Stage__c , &quot;Closed Lost&quot;), ISBLANK( Reason_for_Closed_Lost__c  ) )</errorConditionFormula>
        <errorDisplayField>Reason_for_Closed_Lost__c</errorDisplayField>
        <errorMessage>When the stage is &quot;Closed lost &quot;, the  field &quot;reason for the lost &quot; should not be empty</errorMessage>
    </validationRules>
